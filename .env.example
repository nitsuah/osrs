# .env.example

# Application
# Flask environment: development, production, testing
FLASK_ENV=development

# Database
# PostgreSQL, MySQL, etc.
DATABASE_URL=postgresql://user:password@host:port/database_name
# Example: DATABASE_URL=mysql://user:password@host:port/database_name

# Authentication & Security
SECRET_KEY=your_super_secret_key # Used for session management and signing data

# External APIs
# OSRS API (if any, placeholder)
OSRS_API_KEY=YOUR_OSRS_API_KEY # TODO: Obtain from official OSRS API if available

# Runtime Configuration
# Port for the application to listen on
PORT=5000